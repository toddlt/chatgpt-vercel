{
  "rewrites": [
    {
      "source": "/raw/cron",
      "destination": "/raw/cron"
    },
    {
      "source": "/raw",
      "destination": "/raw"
    },
    {
      "source": "/api",
      "destination": "/api"
    },
    {
      "source": "/:path((?!api).*)",
      "destination": "/"
    }
  ],
  "crons": [
    {
      "path": "/raw/cron",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/raw/cron",
      "schedule": "0 12 * * *"
    }
  ],
  "github": {
    "silent": true
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Credentials", "value": "true" },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type"
        }
      ]
    }
  ]
}
